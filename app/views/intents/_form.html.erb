<div>
  <%= form_for [ @skill, @intent ] do | f |%>
    Name:<br>
    <%= f.text_field :name %><br><br>
    Description:<br>
    <%= f.text_area :description %><br><br>
    Web Hook URL:<br>
    <%= f.text_field :web_hook %><br><br>
    <%= f.submit 'Submit', :class=>"btn md black" %>
  <% end %>
</div>
